      majorValueArr:[],   //专业数组

      levelValue: "",    //显示的楼层
      levelValue_id:'',
      levelValue_show:false,
      levelValueArr:[],   //楼层数组

      positionValue: "", // 详细位置

      drivenValue: "",  // 显示的构件
      drivenValue_id:'',
      drivenValueArr:[], //构件数组
      drivenValue_show:false,

      currentDate: new Date(), //发起时间
