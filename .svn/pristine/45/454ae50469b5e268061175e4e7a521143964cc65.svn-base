      ],
      
       //   工程变更数据
      themeValue: "",// 主题
      themeValueArr:[],
      themeValue_show:false,

      operationCategory_columns: [], //楼栋数组
      operationCategory_show:false,
      operationCategory:'', //显示的楼栋名称
      projectGlass:'',  //要传的楼栋id

      majorValue: "",    //显示的专业
