// 获取楼栋
export const getBuildList = (that, cb) => {
    GetBuilding({ projectid: projectid }).then(res => {
        if (res.StatusCode == 200) {
            if (res.Detiel.length > 0) {
                res.Detiel.unshift({ text: "全部" })
            }
            if (typeof cb == 'function') {
                cb(res.Detiel)
            } else {
                that.BuildNameList = res.Detiel;
            }
        }
    });
}

