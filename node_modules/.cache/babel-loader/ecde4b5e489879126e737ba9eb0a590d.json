ipt type='textecmascript-6'>\r\nexport default {\r\n  data() {\r\n    return {\r\n      all_user_list: [\r\n        { id: 1, text: \"李一一\" },\r\n        { id: 2, text: \"李二二\" },\r\n        { id: 3, text: \"李三三\" },\r\n        { id: 4, text: \"李思思\" },\r\n        { id: 5, text: \"李呜呜\" },\r\n        { id: 6, text: \"李六六\" },\r\n        { id: 7, text: \"李柒柒\" },\r\n        { id: 8, text: \"李叭叭\" },\r\n        { id: 9, text: \"李玖玖\" },\r\n        { id: 10, text: \"李师师\" },\r\n        { id: 11, text: \"李十一\" },\r\n        { id: 12, text: \"李十二\" },\r\n        { id: 13, text: \"李十三\" },\r\n        { id: 14, text: \"李十四\" },\r\n        { id: 15, text: \"李失误\" }\r\n      ], //所有人员列表\r\n      checked: false, //是否勾选\r\n      text: [], //名字\r\n      checkbox: [],//默认选中\r\n      userId: 1000000,\r\n    };\r\n  },\r\n  created() {\r\n    this.active = this.$route.query.active\r\n  },\r\n  methods: {\r\n    //返回\r\n    goback() {\r\n      this.$router.push({path:\"/riskDetails\", query: {active: this.active}});\r\n    },\r\n    //选中\r\n    handleCheck(id) {\r\n      this.text = []\r\n      this.checkbox = id;\r\n      console.log(this.checkbox)\r\n      this.all_user_list.forEach(i => {\r\n        this.checkbox.forEach(j=>{\r\n          if (i.id == j) {\r\n            this.text.push(i.text);\r\n          }\r\n        })\r\n      });\r\n      console.log('text',this.text);\r\n    },\r\n    //确定接收人\r\n    Determine() {\r\n      // this.$router.push({path:\"/riskDetails\", query: {active: this.active, text: this.text}});\r\n      // this.$Dialog\r\n      //   .confirm({\r\n      //     title: \"温馨提示\",\r\n      //     message: \"请确认接收人!\"\r\n      //   })\r\n      //   .then(() => {\r\n      //     // 确定\r\n      //   })\r\n      //   .catch(() => {\r\n      //     // 取消\r\n      //   });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang='stylus' scoped  rel='stylesheet/stylus'>\r\n.container-receiver\r\n  width 100%\r\n  height 100%\r\n  .van-nav-bar\r\n    height 88px\r\n   